
<div class="container">
  <div class="row">
    <div class="col-sm-6 col-sm-offset-3">
     <h2 class="text-center">Log in
    <%= form_tag '/login' do %>

    <%= label :user, :email  %>
    <%= text_field :user, :email %>
    <%= label :user, :password %>
    <%= password_field :user, :password %>
    <%= submit_tag "Login" %></h2>
<% end %>


<h4 class="text-center"><%= button_to 'Back', new_user_path, :method => 'get' %></h4>

   </div>
  </div> 
</div>
</div>